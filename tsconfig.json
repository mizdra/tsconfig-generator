{
  "extends": "./tsconfig.base.json",
  "exclude": ["node_modules", "**/dist"],
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "noEmit": true,
    "rootDirs": [".", "generated"]
  }
}
